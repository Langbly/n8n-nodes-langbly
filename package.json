{
  "name": "n8n-nodes-langbly",
  "version": "0.1.0",
  "description": "n8n community node for the Langbly translation API. Context-aware translation, Google Translate v2 compatible, 5-10x cheaper.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "translation",
    "langbly",
    "i18n",
    "localization",
    "machine-translation"
  ],
  "license": "MIT",
  "homepage": "https://langbly.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Langbly/n8n-nodes-langbly"
  },
  "main": "dist/nodes/Langbly/Langbly.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Langbly/Langbly.node.js"
    ],
    "credentials": [
      "dist/credentials/LangblyApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.20.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
